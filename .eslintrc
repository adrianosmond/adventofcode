{
  "parserOptions": {
    "ecmaVersion": 2019,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-plusplus": "off",
    "no-console": "off",
    "no-restricted-syntax": "off",
    "max-classes-per-file": "off"
  }
}
