{
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "jest"],
  "env": {
    "jest": true
  },
  "rules": {
    "prettier/prettier": ["error"],
    "no-plusplus": "off",
    "no-console": "off",
    "no-restricted-syntax": "off",
    "max-classes-per-file": "off",
    "no-continue": "off"
  }
}
